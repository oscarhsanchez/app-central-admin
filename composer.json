{
    "name": "esocial/vallas.admin",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "dev-master",
        "esocial/esocial-admin-bundle": "dev-devel",
        "esocial/esocial-model-bundle": "dev-devel",
        "esocial/esocial-utils-bundle": "dev-devel",
        "esocial/esocial-security-bundle": "dev-master",
        "vallas/model-bundle": "dev-master",
        "jms/translation-bundle": "dev-master",
        "jms/di-extra-bundle": "dev-master",
        "knplabs/knp-paginator-bundle": "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "symfony/phpunit-bridge": "~2.7",
        "doctrine/data-fixtures": "^1.1",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "hautelook/alice-bundle": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
  "repositories" : [
      {
          "type" : "vcs",
          "url" : "git@bitbucket.org:esocialtech/esocial-admin-bundle.git"
      },{
          "type" : "vcs",
          "url" : "git@bitbucket.org:esocialtech/esocial-model-bundle.git"
      },{
          "type" : "vcs",
          "url" : "git@bitbucket.org:esocialtech/esocial-utils-bundle.git"
      },{
          "type" : "vcs",
          "url" : "git@bitbucket.org:esocialtech/esocial-security-bundle.git"
      },{
          "type" : "vcs",
          "url" : "git@bitbucket.org:vallas/vallas-model-bundle.git"
      }
  ],
  "minimum-stability": "dev"
}
