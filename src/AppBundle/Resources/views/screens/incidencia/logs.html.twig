<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>{{ 'form.incidencia.label.user'|trans }}</th>
            <th>{{ 'form.incidencia.label.fecha'|trans }}</th>
            <th>{{ 'form.incidencia.label.accion'|trans }}</th>
        </tr>
    </thead>
    <tbody>
        {% for log in entity.logs %}
        <tr>
            <td>{{ log.codigoUser }}</td>
            <td>{{ log.fecha|dateToFormat }}</td>
            <td>{{ log.accion }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>