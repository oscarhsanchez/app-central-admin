{% set selectFunction = getVars.selectFunction is defined and getVars.selectFunction is not empty ? getVars.selectFunction : 'selectCategoriaPropuesta' %}
<table id="dt-categoria-propuesta-list-select" class="table table-striped table-bordered table-hover">
    <thead>
    <tr>
        <th>{{ 'form.categoria_propuesta.label.nombre'|trans }}</th>
        <th>{{ 'form.label.actions'|trans }}</th>
    </tr>
    </thead>
</table>
<script type="text/javascript">
    var categoria_propuesta_list_select = null;
    var categoria_propuesta_list_select_table_columns = [
        {"mData": "nombre"},
        {
            "mData": function (source) {
                return '<a class="btn btn-sm btn-primary" href="javascript:void(0)" onclick="closeThisPopup(this); {{ selectFunction }}(\''+encodeURIComponent(JSON.stringify(source))+'\', \'{{ getVars|json_encode|escape  }}\')">Select</a>';
            }
        }
    ];
    categoria_propuesta_list_select = TableManaged.init(
            '#dt-categoria-propuesta-list-select',
            '{{ url('categoria_propuesta_list_json', getVars) }}',
            null,
            [],
            categoria_propuesta_list_select_table_columns,
            '{{ url('datatables_i18n_fragment', { language: app.request.locale }) }}'
    );
</script>